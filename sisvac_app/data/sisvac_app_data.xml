<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Switch root menu "Invoicing" to "Accounting" -->
    <!-- Top menu item -->
    <menuitem name="SISVAC"
              id="menu_sisvac_root"
              web_icon="sisvac_app,static/description/icon.png"
              sequence="40"/>
    <!-- move existing submenus to point to the new parent -->
    <record id="sisvac_appointments.sisvac_primary_menu_vaccination" model="ir.ui.menu">
        <field name="parent_id" ref="menu_sisvac_root"/>
    </record>
    <record id="sisvac_stock.sisvac_primary_menu_vaccines" model="ir.ui.menu">
        <field name="parent_id" ref="menu_sisvac_root"/>
    </record>

</odoo>
